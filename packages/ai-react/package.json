{
  "name": "@tanstack/ai-react",
  "version": "0.1.0",
  "description": "React hooks for TanStack AI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "react",
    "hooks",
    "tanstack",
    "chat",
    "streaming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tanstack/ai": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@tanstack/ai": "workspace:*",
    "react": ">=18.0.0"
  }
}
