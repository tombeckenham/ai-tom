{
  "name": "@tanstack/ai-ollama",
  "version": "0.5.0",
  "description": "Ollama adapter for TanStack AI",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/ai.git",
    "directory": "packages/typescript/ai-ollama"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
